#-----------------------------JARVIS
JARVIS_HOME=JARVIS_INSTALL_DIR
export JARVIS_HOME
JARVIS_APP=${JARVIS_HOME}/app
export JARVIS_APP
JARVIS_LOG=${JARVIS_HOME}/Jarvis.log
export JARVIS_LOG
JARVIS_OLD_LOG=${JARVIS_HOME}/old.Jarvis.log
export JARVIS_OLD_LOG

PATH=${PATH}:${JARVIS_HOME}
export PATH

alias startJarvis="jarvisServer start"
alias stopJarvis="jarvisServer stop"
alias restartJarvis="jarvisServer restart"


